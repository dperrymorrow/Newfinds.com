- blog.articles[0..5].each do |article|
  %article
    %h1
      = link_to article.title, article.url
      %time= article.date.strftime('%b %e %Y')

    %p
      = article_summary article
      = link_to 'Read More', article.url

%hr

- project_articles.each do |article|
  %article
    %h1= article.title